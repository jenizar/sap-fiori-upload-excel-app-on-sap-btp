@AccessControl.authorizationCheck: #NOT_REQUIRED
@EndUserText.label: 'Projection Entity - Excel Filename'
@Metadata.ignorePropagatedAnnotations: true
@Metadata.allowExtensions: true
define root view entity YUPL_C_XL_FILE 
provider contract transactional_query
as projection on YUPL_I_XL_FILE
{
    key EndUser,
    key FileId,
    FileStatus,
      @Semantics.largeObject:
            { mimeType: 'Mimetype',
            fileName: 'Filename',
            acceptableMimeTypes: [ 'application/vnd.ms-excel','application/vnd.openxmlformats-officedocument.spreadsheetml.sheet' ],
            contentDispositionPreference: #INLINE } 
    Attachment,
    Mimetype,
    Filename,
    LocalCreatedBy,
    LocalCreatedAt,
    LocalLastChangedBy,
    LocalLastChangedAt,
    LastChangedAt,
    Status_msg,
    /* Associations */
    _XLData: redirected to composition child YUPL_C_XL_DATA
}

